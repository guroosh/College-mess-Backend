create table users
(
	userId varchar(100),
	primary key(userId)
);
	
create table posts
(
	id int not null auto_increment,
	userId varchar(100),
	text varchar(400),
	imageURL varchar(200),
	userImageURL varchar(200),
	upVotes int,
	date varchar(100),
	primary key (id)
);

create table comments
(
	id int not null auto_increment,
	postId int,
	userId varchar(100),
	date varchar(100),
	text varchar(400),
	primary key (id)
);

create table likes
(
	userId varchar(100),
	postId int
);

create table hidden
(
	userID varchar(100), 
	postId int
);


create table menu_items
(
	name varchar(100) not null,
	up int,
	down int,
	primary key (name)
);

create table day_meal
(
	name varchar(100),
	day int,
	meal varchar(40)
);

create table time
(
	meal varchar(40),
	start_time varchar(40),
	end_time varchar(40)
);

create table links
(
    mailing varchar(80),
    feedback varchar(200)
);
create table empty
(
    id int,
    primary key (id)
);

